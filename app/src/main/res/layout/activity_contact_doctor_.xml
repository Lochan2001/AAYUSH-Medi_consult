<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".contact_doctor_Activity">

    <ImageView
        android:id="@+id/contactdoc"
        android:layout_width="411dp"
        android:layout_height="310dp"
        android:layout_marginTop="28dp"
        android:background="#E4DEDE"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/doc_log" />

    <TextView
        android:id="@+id/contacttodoc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/linkdoc"
        android:clickable="true"
        android:textSize="30sp"
        android:background="#FFEB3B"
        android:textColor="#830E0E"
        android:textAlignment="center"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginTop="70dp"
        app:layout_constraintTop_toBottomOf="@id/contactdoc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="372dp"
        android:gravity="center_horizontal" />
</androidx.constraintlayout.widget.ConstraintLayout>